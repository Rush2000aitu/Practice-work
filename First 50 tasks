27. #include <bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c;
    cin >>a>>b>>c;
    int d=a;int e=b;int f=c;
    a=d+e;
    b=f-d;
    c=d+e+f;
    cout <<a<<" "<<b<<" "<<c;
}
