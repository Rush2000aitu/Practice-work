46. #include <bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c,max=0;
    cin >>a>>b>>c;
    if (a%5==0&&b%5==0&&c%5==0)
    cout << a+b+c;
    else if (a%5==0&&b%5==0)
    cout <<a+b;
    else if (b%5==0&&c%5==0)
    cout <<b+c;
    else if (a%5==0&&c%5==0)
    cout <<a+c;
    else if (a%5==0)cout <<a;
    else if (b%5==0)cout <<b;
    else if (c%5==0)cout <<c;
    else if (a%5!=0&&b%5!=0&&c%5!=0)
    cout <<"error";
}
47. #include <bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c,max=0;
    cin >>a>>b>>c;
    if (a>b&&a>c)cout << a;
    else if (b>c&&b>a)cout << b;
    else if (c>a&&c>b)cout << c;
}
48. #include <bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c,max=0;
    cin >>a>>b>>c;
    if (a+b>max) max = a+b;
    if (a+c>max) max = a+c;
    if (c+b>max) max = c+b;
    if (max==a+b)cout << a<<" "<<b;
    if (max==a+c)cout << a<<" "<<c;
    if (max==c+b)cout << c<<" "<<b;
}
49. #include <bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c,d,max=0;
    cin >>a>>b>>c>>d;
    if (a%2==0||b%2==0||c%2==0||d%2==0){
        if (a%2==0)
            max = a;
         if (b%2 == 0 && b>max)
            max = b;
         if (c%2 == 0 && c>max)
            max = c;
         if (d%2 == 0 && d>max)
            max = d;
    }
     else {
        cout <<"Not found";
    }
    cout << max;
}
