3. #include <bits/stdc++.h>
using namespace std;
int main(){
	for ( int i = 1 ; i <= 5; i++){
		for ( int j = 0; j < i; j++)
			cout << "0" << " ";
			cout << endl;
	}
}
4. #include <bits/stdc++.h>
using namespace std;
int main(){
	for ( int i = 0; i < 5; i++){
		for ( int j = 0; j < 8; j++){
